{
    source: {
        files: [
            "nginx.log",
        ],
    },

    parser: {
        num_threads: 2,
        sample_rate: 2,
        engine: {
            nginx: {
                config_file: "nginx.conf",
                log_format_name: "combined",
            },
        },
    },

    filter: [
        ["add", "goose", "man"],
        ["drop", "remote_user"],
        ["timestamp", "goose", "%s"],
        ["http_request_line", "request", {
            "extract_query_args": "all",
        }],
    ],

    uploader: {
        data_set: "LoadBalancer",
        libhoney_config: {
            max_concurrent_batches: 12,
        },
    },
}
